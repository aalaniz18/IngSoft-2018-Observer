import java.util.HashMap;

public class BaseDeDatos {
	HashMap<String,String> admin;
	HashMap<String, String> empleado;
	
	public BaseDeDatos(){
	admin= new HashMap<String, String>(1);
	empleado= new HashMap<String,String>();
	}
	
	public void addEmpleado(Empleado e){
		empleado.put(e.getPass(),e.getUsuario());
	}
	
	public void addAdministrador(Administrador a){
		admin.put(a.getPass(),a.getUsuario());
	}
}
