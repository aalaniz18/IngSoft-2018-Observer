
public class Entrada extends Producto{
	String pelicula;
	String ubicacion;
	
	public Entrada(){
		super();
	}
	
	public void setNombreP(String n){
		pelicula=n;}
	
	public String getNombreP(){
		return pelicula;}
	
	public void setUbicacion(String u){
		ubicacion=u;}
	
	public String getUbicacion(){
		return ubicacion;}

}
